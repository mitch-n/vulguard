{

  "manifest_version": 2,
  "name": "Vulguard Profanity Filter",
  "version": "1.0",

  "description": "Filters Youtube Videos",
  
  "icons": {
    "48": "icons/vulguard-icon.svg",
	"96": "icons/vulguard-icon.svg"
  },
  
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["YoutubeProfanityFilter-v3.js"]
    },
	{
      "matches": ["*://*.netflix.com/*"],
      "js": ["NetflixProfanityFilter.js"]
    },
	{
      "matches": ["*://*.disneyplus.com/*"],
      "js": ["DisneyProfanityFilter.js"]
    },
	{
      "matches": ["*://*.hulu.com/*"],
      "js": ["HuluProfanityFilter.js"]
    },
	{
      "matches": ["*://*.amazon.com/*"],
      "js": ["AmazonProfanityFilter.js"]
    }
  ]

}
